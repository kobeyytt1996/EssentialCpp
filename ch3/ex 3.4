using namespace std;

class even_elem {
    public:
    bool operator()(int elem)
    { return elem%2 ? false : true; }
};

int main()
{
    ofstream even_file("even_file.txt"),
            odd_file("odd_file.txt");

    vector<int> inputt;
    istream_iterator<int> in(cin), eos;
    ostream_iterator<int> even_iter(even_file, "\n"),
    odd_iter(odd_file, " ");

    copy(in, eos, back_inserter(inputt));


vector<int>::iterator division = partition(inputt.begin(), inputt.end(), even_elem());


copy(inputt.begin(), division, even_iter);
copy(division, inputt.end(), odd_iter);
}

